.top {
	text-shadow: none !important;
	color: white;
	line-height: 50px;
	text-align: center;
	height: 50px;
	width: 100%;
	background-color: #424458;
	background-size: 100% 100%;
	background-repeat: no-repeat;
	display: -webkit-flex;
	display: flex;
	-webkit-align-items: center;
	align-items: center;
}

.showframe {
	width: 100%;
	height: 100%;
	border: none;
	display: none;
}

.back_ic {
	height: 24px;
